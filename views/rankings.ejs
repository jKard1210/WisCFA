<!doctype html>


<html>
    <head>
        <link rel='stylesheet' href='/styles/homePage.css' />
        <title>Wisconsin College Football Analytics</title>
        <style>
          tr {
            font-size: 20px;
            font-weight: bold;
            text-align: center;
             border-bottom: solid 2px #FFF;
             border-radius: 5px;
          }
          
          th {
            font-size: 30px;
            margin-bottom: 30px;
            text-align: center;
            color: white;
          }
          
          table {
            border-collapse: collapse;
          }
        </style>
    </head>
    

    
<div class = "container" style = "width: 500px">
  <h1 style = "font-size: 100px; color: white; font-family: 'Open Sans'; text-align: center; margin-bottom: 5px">WISCFA</h1>  
  <h1 style = "font-size: 25px; color: black; font-family: 'Open Sans'; text-align: center">Wisconsin College Football Analytics</h1>  
  <div class="wrap" style = "margin-bottom: 40px">
    <a href="/">
      <div class="first">Back</div>
      <div class="second">Return to Homepage</div>
    </a>
  </div>
  
  <h1 style = "margin-top: 30px; margin-bottom: 5px; font-family: 'Open Sans'">2016 Final Colley Ratings</h1>
  <h1 style = "margin-top: 10px; margin-bottom: 5px; font-family: 'Open Sans'; font-size: 15px; font-weight: normal">2017 Rankings Will Be Available Following Week 3</h1>
<table style = "width: 500px;">
  <tr><td style = "text-align: left; font-size: 20px; color: #fff">Team Name</td><td style = "font-size: 20px; color: #fff">Colley Score</td></tr>
  <% entries = data.map(function(elem) {
    return '<tr><td style = "text-align: left">' + elem.teams1 + '</td><td>' + (Math.round(elem.teams2*100)/100) + '</td></tr>';
}) -%>
<p><%- entries.join("") %></p>

</table>
<div class="wrap" style = "margin-top: 30px">
    <a href="/about">
      <div class="first">About</div>
      <div class="second">Methodology</div>
    </a>
  </div>
</div>


</html>
